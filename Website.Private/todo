else if(ancestral.parentElement instanceof Element) {
            if(ancestral.parentElement.hasAttribute("editor-editable")) {
                const selectedTag = ancestral.parentElement.closest("[editor-tag]");
                if(!selectedTag) return;
                const text = ancestral.parentElement.textContent || ""

                const selectedContent = direction > 0
                    ? text.slice(selection.anchorOffset, selection.focusOffset + 1)
                    : text.slice(selection.focusOffset, selection.anchorOffset + 1);

                const tagSelections = [{
                    element: selectedTag,
                    content: selectedContent,
                    contentFormat: ContentFormat.Selected
                }] satisfies TagSelection[]

                console.log(tagSelections)
            }
        }